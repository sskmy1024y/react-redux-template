{
  "name": "react-template",
  "author": "sho",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "webpack-dev-server --port 3000 ",
    "build": "webpack",
    "watch": "webpack -w --config webpack.config.js",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prettier": "prettier --check './src/**/*.{ts,tsx}'",
    "eslint": "eslint --ext .ts,.tsx,.js,.jsx,.mjs ./src/",
    "eslint:fix": "yarn run eslint --fix",
    "stylelint": "stylelint --cache --cache-location node_modules/.cache/stylelint/ './src/**/*.{css,scss}'",
    "stylelint:fix": "yarn run stylelint --fix",
    "stylelint:styled": "stylelint --config ./.stylelintrc.styled.json --cache --cache-location node_modules/.cache/stylelint/ --ip '*.d.ts' './src/**/*.[jt]s{,x}'",
    "stylelint:styled:fix": "yarn run stylelint:styled --fix",
    "lint": "yarn run stylelint && yarn run stylelint:styled",
    "lint:fix": "yarn run stylelint:fix && yarn run stylelint:styled:fix && yarn run prettier:fix",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-storyshots": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^25.1.4",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "jest": "^25.1.0",
    "jest-resolve": "^25.1.0",
    "jest-snapshot": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpackbar": "^4.0.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
